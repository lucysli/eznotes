<% @user = current_user %>


<header class="span12 header">
   <h4 class="center span4">FALL</h4>
   <h4 class="center span4">WINTER</h4>
   <h4 class="center span4">SUMMER</h4>
</header>
<section class="row-fluid body">
   <ul class="span4 fall">
      <%= render partial: 'shared/course_feed_item', collection: @fall_course_feed_items %>
   </ul>

   <ul class="span4 winter">
      <%= render partial: 'shared/course_feed_item', collection: @winter_course_feed_items %>
   </ul>

   <ul class="span4 summer">
      <%= render partial: 'shared/course_feed_item', collection: @summer_course_feed_items %>
   </ul>
</section>





