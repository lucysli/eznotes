<dl id="<%= course.id %>" class="course">
   <dt>
      <%= link_to course.subject_code + 
          " #{course.course_num}" + 
          " #{course.section}", course %> (<%= course.term.upcase %>)
   </dt> 
   <dd>
      <%= course.course_title %>
      <% if current_user.admin? %>
         <%= link_to "delete", course, method: :delete,
             data: { confirm: "Delete Course: 
              #{course.subject_code} #{course.course_num} #{course.section} #{course.course_title}" }, class: "cus-trash delete" %>
      <% end %>

   </dd>
</dl>

