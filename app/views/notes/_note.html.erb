<li class="span8">
   <span class="notes">
      <h4> Lecture Title: </h4> <%= note.lecture_title %>
      <h4> Lecture Date: </h4> <%= note.lecture_date %>
      <span class="comments">
        <h4> Comments: </h4> <%= note.comments %>
      </span> 
      <span class="offset4">
      <%= link_to('download file', note.file.url, class: "cus-download download", alt: 'download', target: "_blank") %>
      </span> 
   </span>
   <span class="timestamp">
      Uploaded <%= time_ago_in_words(note.created_at) %> ago.
   </span>
   <% if current_user?(note.user) %>
      <%= render partial: 'shared/delete_note_link', locals: { object: note } %>
   <% end %>
</li>